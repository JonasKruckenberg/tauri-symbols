{
	"$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/tooling/cli/schema.json",
	"build": {
		"beforeBuildCommand": "pnpm vite build && rm build/vite-manifest.json",
		"beforeDevCommand": "pnpm vite dev",
		"devPath": "http://localhost:5173",
		"distDir": "../build",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "Tauri-Symbols"
	},
	"tauri": {
		"pattern": {
			"use": "isolation",
			"options": {
			  "dir": "../isolation"
			}
		  },
		"allowlist": {
			"all": false,
			"window": {
				"show": true,
				"startDragging": true,
				"maximize": true,
				"unmaximize": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.tauri.symbols",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": ["app", "dmg"],
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": {
				"default-src": "'none'",
				"style-src": "'self'"
			},
			"freezePrototype": true
		},
		"updater": {
			"active": false
		},
		"windows": [{
			"width": 1000,
			"height": 800,
			"minWidth": 500,
			"minHeight": 500,
			"resizable": true,
			"fullscreen": false
		}]
	}
}
