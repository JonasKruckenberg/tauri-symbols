{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "pnpm vite build && rm build/vite-manifest.json",
		"beforeDevCommand": "pnpm vite dev",
		"devPath": "http://localhost:5173",
		"distDir": "../build",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "Tauri Symbols",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"window": {
				"show": true,
				"startDragging": true,
				"maximize": true,
				"unmaximize": true
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "com.tauri.symbols",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "app",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": {
				"default-src": "'none'",
				"style-src": "'self'"
			},
			"freezePrototype": true
		},
		"updater": {
			"active": false
		},
		"windows": []
	}
}
